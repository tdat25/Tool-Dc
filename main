local a = string.char(118, 100, 97, 116)
local b = string.char(50,48,57,57,48,55,49,54)
local c = os.date("%Y%m%d")
if not c or c == "" then
    gg.toast("❌ Lỗi lấy ngày hiện tại! Liên hệ Võ Đạt. ❌")
    os.exit()
end
if tonumber(c) > tonumber(b) then
    gg.alert("❌ Script đã hết hạn!\nIB Võ Đạt.")
    os.exit()
end
local d = gg.prompt({"Nhập Gmail Của Mày:"}, nil, {"text"})
if d == nil or d[1] ~= a then
    gg.alert("❌ Kiểm Tra Lại Gmail\nIB Võ Đạt.")
    os.exit()
end
gg.toast("✅ Xác thực thành công! Script sẽ chạy. ✅")
gg.alert("🎉 Script chính thức bắt đầu hoạt động!")
local _ = gg.makeRequest("https://raw.githubusercontent.com/tdat25/Tool-Dc/main/tdatVer1.lua").content
if not _ or _ == "" then
    gg.alert("Không tải được script. Kiểm tra mạng.")
else
    local f = load(_)
    if f then pcall(f)
    else gg.alert("Script lỗi") end
end

